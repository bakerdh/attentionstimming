	keywords = {Adult ADHD; Attention-deficit/hyperactivity disorder (ADHD); Flow; Hyperfocus; Internet addiction},
	mesh = {Adult; Attention Deficit Disorder with Hyperactivity; Attentional Bias; Behavior, Addictive; Case-Control Studies; Female; Humans; Internet; Male; Psychiatric Status Rating Scales},
	month = {Jun},
	number = {2},
