	journal = {NeuroImage},
	keywords = {Visual Attention, Spatial orienting, Functional MRI, Event-related fMRI, Endogenous, Exogenous, Human},
	number = {2},
	pages = {822-830},
	title = {Endogenous and exogenous attention shifts are mediated by the same large-scale neural network},
	url = {https://www.sciencedirect.com/science/article/pii/S1053811904000801},
