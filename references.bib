	pages = {100897},
	title = {Rethinking Stereotypies in Autism},
	url = {https://www.sciencedirect.com/science/article/pii/S1071909121000255},
	volume = {38},
	year = {2021},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S1071909121000255},
