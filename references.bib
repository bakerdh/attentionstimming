@article{Peelen2004,
	abstract = {Event-related fMRI was used to examine the neural basis of endogenous (top-down) and exogenous (bottom-up) spatial orienting. Shifts of attention were induced by central (endogenous) or peripheral (exogenous) cues. Reaction times on subsequently presented targets showed the expected pattern of facilitation and inhibition in both conditions. No difference in brain activity was observed when the two orienting conditions were contrasted with a liberal threshold, showing that both forms of orienting were mediated by the same neural network. Compared to within-block control trials, both endogenous and exogenous orienting activated a fronto-parietal network consisting of premotor cortex, posterior parietal cortex, medial frontal cortex and right inferior frontal cortex. Within these regions, equally strong activation was observed for both orienting conditions. It is concluded that endogenous and exogenous orienting are mediated by the same large-scale network of frontal and parietal brain areas.},
	author = {Marius V Peelen and Dirk J Heslenfeld and Jan Theeuwes},
	date-added = {2024-07-03 15:37:02 +0100},
	date-modified = {2024-07-03 15:38:58 +0100},
